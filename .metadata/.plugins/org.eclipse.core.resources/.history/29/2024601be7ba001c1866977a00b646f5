package Study;

public class Array {
	void arrayinsert(int a[]) {
		//a[0]=0;
		for(int i=1;i<11;i++) {
			
			a[i]=i*10;
			System.out.print(a[i]+" ");
		}
		System.out.println();
	}
	void stringArray() {
		String a[]= {"zee","hello","tech"};
		for(int i=0;i<a.length;i++) {
			System.out.print(a[i]+" ");
		}
		
	}
	public static void main(String[] args) {
		int b[]=new int[11];
		Array array=new Array();
		array.arrayinsert(b);
		array.stringArray();
	}

}
