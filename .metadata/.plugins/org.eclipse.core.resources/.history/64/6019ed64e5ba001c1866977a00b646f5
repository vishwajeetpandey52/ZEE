package Study;

public class Array {
	void arrayinsert(int a[]) {
		a[0]=0;
		for(int i=0;i<10;i++) {
			
			a[i]=a[i]+10;
			
		}
		for(int i=0;i<10;i++) {
			System.out.println(a[i]);
		}
	}
	public static void main(String[] args) {
		int b[]=new int[10];
		Array array=new Array();
		array.arrayinsert(b);
	}

}
